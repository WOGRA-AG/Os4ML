{
  "name": "os4ml",
  "version": "0.0.0",
  "scripts": {
    "check-env": "node -e 'console.log(process.env)' | grep npm",
    "generate:base": "openapi-generator-cli generate -g typescript-angular --generator-key v3.0 --additional-properties=ngVersion=13.0.0",
    "generate:run": "npm run generate:base -- -i $npm_config_kfp_single_file -o ./build/openapi/run",
    "generate:task": "npm run generate:base -- -i $npm_config_task -o ./build/openapi/task",
    "generate:kfp": "npm run generate:base -- -i $npm_config_kfp -o ./build/openapi/kfp",
    "generate:kfp-upload": "npm run generate:base -- -i $npm_config_kfp_upload -o ./build/openapi/kfp-upload",
    "generate:kfp-single-file": "npm run generate:base -- -i $npm_config_kfp_single_file -o ./build/openapi/kfp-single-file",
    "generate:job": "npm run generate:base -- -i $npm_config_job -o ./build/openapi/job",
    "generate:auth": "npm run generate:base -- -i $npm_config_auth -o ./build/openapi/auth",
    "generate:experiment": "npm run generate:base -- -i $npm_config_experiment -o ./build/openapi/experiment",
    "generate:objectstore": "npm run generate:base -- -i ../objectstore-manager/oas/objectstore-manager-oas.yaml -o ./build/openapi/objectstore",
    "generate:api": "npm run generate:run & npm run generate:task & npm run generate:kfp & npm run generate:kfp-upload & npm run generate:kfp-single-file & npm run generate:job & npm run generate:job & npm run generate:auth & npm run generate:experiment & npm run generate:objectstore",
    "prestart": "npm run generate:api",
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm run generate:api",
    "build": "ng build --base-href /os4ml/",
    "watch": "ng build --base-href /os4ml/ --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@openapitools/openapi-generator-cli": "^2.4.25",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.3"
  }
}
