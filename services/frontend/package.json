{
  "name": "os4ml",
  "version": "0.0.0",
  "scripts": {
    "check-env": "node -e 'console.log(process.env)' | grep npm",
    "generate:base": "openapi-generator-cli generate -g typescript-angular --generator-key v3.0 --additional-properties=ngVersion=13.0.0",
    "generate:modelmanager": "npm run generate:base -- -i ../oas/model-manager-oas.yaml -o build/openapi/modelmanager",
    "prestart": "npm run generate:modelmanager",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check true --proxy-config proxy.conf.json",
    "start-frontend": "ng serve --host 0.0.0.0 --disable-host-check true --proxy-config frontend-only-proxy.conf.json",
    "prebuild": "npm run generate:modelmanager",
    "prepare": "cd ../.. && husky install ./services/frontend/.husky",
    "build": "ng build --output-hashing=all --base-href .",
    "watch": "ng build --base-href . --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint && prettier --check .",
    "format": "ng lint --fix && prettier --write .",
    "prettier": "prettier --write .",
    "e2e:run": "npx cypress run",
    "e2e:local": "cross-env CYPRESS_dev=true npx cypress run",
    "cypress:open": "cross-env CYPRESS_dev=true npx cypress open",
    "cypress:open-test": "npx cypress open",
    "config-prettier": "eslint-config-prettier src/main.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.0",
    "@angular/cdk": "16.2.0",
    "@angular/common": "16.2.0",
    "@angular/compiler": "16.2.0",
    "@angular/core": "16.2.0",
    "@angular/forms": "16.2.0",
    "@angular/material": "16.2.0",
    "@angular/platform-browser": "16.2.0",
    "@angular/platform-browser-dynamic": "16.2.0",
    "@angular/router": "16.2.0",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "lottie-web": "^5.12.2",
    "ngx-lottie": "^10.0.0",
    "rxjs": "7.4.0",
    "tslib": "2.6.2",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.0",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "^16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "16.2.0",
    "@angular/compiler-cli": "16.2.0",
    "@openapitools/openapi-generator-cli": "2.5.2",
    "@types/jasmine": "4.3.5",
    "@types/node": "12.20.55",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "cross-env": "7.0.3",
    "cypress": "12.7.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsdoc": "46.4.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-rxjs-angular": "2.0.1",
    "husky": "8.0.3",
    "jasmine-core": "4.5.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "prettier": "^3.0.2",
    "prettier-eslint": "^15.0.1",
    "typescript": "5.1"
  }
}
