{
  "name": "os4ml",
  "version": "0.0.0",
  "scripts": {
    "check-env": "node -e 'console.log(process.env)' | grep npm",
    "generate:base": "openapi-generator-cli generate -g typescript-angular --generator-key v3.0 --additional-properties=ngVersion=13.0.0",
    "generate:modelmanager": "npm run generate:base -- -i ../oas/model-manager-oas.yaml -o build/openapi/modelmanager",
    "prestart": "npm run generate:modelmanager",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check true --proxy-config proxy.conf.json",
    "start-frontend": "ng serve --host 0.0.0.0 --disable-host-check true --proxy-config frontend-only-proxy.conf.json",
    "prebuild": "npm run generate:modelmanager",
    "prepare": "cd ../.. && husky install ./services/frontend/.husky",
    "build": "ng build --output-hashing=all --base-href .",
    "watch": "ng build --base-href . --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint && prettier --check .",
    "format": "ng lint --fix && prettier --write .",
    "prettier": "prettier --write .",
    "config-prettier": "eslint-config-prettier src/main.ts",
    "e2e:visually:local": "cross-env CYPRESS_dev=true npx cypress open",
    "e2e:visually:local:fast": "cross-env CYPRESS_dev=true CYPRESS_createEssentialMLEntitiesOnly=true npx cypress open",
    "e2e:visually:testing": "npx cypress open",
    "e2e:visually:testing:fast": "cross-env CYPRESS_createEssentialMLEntitiesOnly=true npx cypress open",
    "e2e:cli:local": "npx cypress run  CYPRESS_dev=false CYPRESS_dev=true CYPRESS_createEssentialMLEntitiesOnly=true --spec 'cypress/e2e/**' --browser chrome",
    "e2e:cli:local:fast": "cross-env CYPRESS_dev=true npx cypress run --spec 'cypress/e2e/**' --browser chrome",
    "e2e:cli:testing": "npx cypress run --spec 'cypress/e2e/**' --browser chrome",
    "e2e:cli:testing:fast": "npx cypress run  CYPRESS_createEssentialMLEntitiesOnly=true --spec 'cypress/e2e/**' --browser chrome",
    "i18n": "ng extract-i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.4",
    "@angular/cdk": "17.3.4",
    "@angular/common": "17.3.4",
    "@angular/compiler": "17.3.4",
    "@angular/core": "17.3.4",
    "@angular/forms": "17.3.4",
    "@angular/material": "17.3.4",
    "@angular/platform-browser": "17.3.4",
    "@angular/platform-browser-dynamic": "17.3.4",
    "@angular/router": "17.3.4",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "cypress-log-to-output": "1.1.2",
    "lottie-web": "5.12.2",
    "ngx-lottie": "11.0.2",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.4",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "17.3.4",
    "@angular/compiler-cli": "17.3.4",
    "@angular/localize": "^17.3.4",
    "@openapitools/openapi-generator-cli": "^2.13.1",
    "@testing-library/cypress": "10.0.1",
    "@types/jasmine": "4.3.6",
    "@types/node": "18.17.19",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "axe-core": "4.8.2",
    "cross-env": "7.0.3",
    "cypress": "13.2.0",
    "cypress-axe": "1.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "46.6.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-rxjs-angular": "2.0.1",
    "husky": "8.0.3",
    "jasmine-core": "5.1.1",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.0.3",
    "prettier-eslint": "15.0.1",
    "typescript": "5.4.5"
  }
}
